{
  "ref": "fedora/22/x86_64",
  "repos": ["fedora-22", "fedora-22-updates"],
  "selinux": true,
  "bootstrap_packages": ["glibc", "nss-altfiles"],
  "install-langs": ["en"],
  "packages": [
    "dhcp-client",
    "dnf",
    "initscripts",
    "iproute",
    "kernel-core",
    "openssh-clients",
    "openssh-server",
    "ostree-grub2",
    "passwd",
    "rpm-ostree",
    "selinux-policy-targeted",
    "sudo",
    "which"
  ],
  "units": [
    "network.service"
  ],
  "postprocess-script": "fedora-22-x86_64.sh"
}
