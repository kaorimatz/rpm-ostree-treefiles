{
  "ref": "fedora/21/x86_64",
  "repos": ["fedora-21", "fedora-21-updates"],
  "selinux": true,
  "bootstrap_packages": ["glibc", "nss-altfiles"],
  "install-langs": ["en"],
  "packages": [
    "dhclient",
    "initscripts",
    "iproute",
    "kernel-core",
    "openssh-clients",
    "openssh-server",
    "ostree-grub2",
    "passwd",
    "rpm-ostree",
    "selinux-policy-targeted",
    "sudo",
    "which",
    "yum"
  ],
  "units": [
    "network.service"
  ],
  "postprocess-script": "fedora-21-x86_64.sh"
}
